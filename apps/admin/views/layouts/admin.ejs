<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <!-- It's Admin Page - block robots -->
    <meta name="robots" content="noindex">

    <title><%= pageTitle %></title>

    <!-- Import Google Icon Font -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Import materialize.css -->
<!--    <link type="text/css" rel="stylesheet" href="/css/materialize.css"  media="screen,projection">-->
    <!-- Import page.css -->
<!--    <link type="text/css" rel="stylesheet" href="<%= pageCss %>"  media="screen,projection">-->
    <!-- Inline style for start -->
    <style>
        html,body{height:100%;margin:0}.page-starter{margin:0;min-height:100%;filter:blur(8px);-webkit-filter:blur(8px)}.page-starter>*{display:none !important}.cover-full-image-1{background-position:center center;background-repeat:no-repeat;background-attachment:fixed;background-size:cover;background-color:#999;background-image:url("/images/landscape/iceland-small.jpg")}
    </style>
</head>
<body>
    <div class="cover-full-image-1 page-starter my-wrapper valign-wrapper">
        <%- body %>
    </div>

    <!-- Import page.css -->
    <link type="text/css" rel="stylesheet" href="<%= pageCss %>"  media="screen,projection">
    <!-- JavaScript at end of body for optimized loading -->
    <script type="text/javascript" src="/js/materialize.min.js" async defer></script>
</body>
</html>
